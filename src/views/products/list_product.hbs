<div id="layoutSidenav_content">
    <main>
        <div class="container" style="margin-top: 70px;">
            <table class="table table-hover">
                <thead>
                <tr>
                    <th>Tên cửa hàng</th>
                    <th>Số điện thoại</th>
                    <th>Ngày đăng kí</th>
                    <th>Trạng thái</th>
                </tr>
                </thead>
                <tbody>
                {{#each listProduct as |item i|}}
                    <tr>
                        <td>
                            <div>{{this.name}}</div>
                        </td>
                        <td>
                            <div>{{this.phone}}</div>
                        </td>
                        <td>
                            <div>{{this.date}}</div>
                        </td>
                        <td>{{this.status}}</td>
                    </tr>
                {{/each}}
                </tbody>
            </table>
        </div>
    </main>
</div>
</div>
