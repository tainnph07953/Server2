    $('#thanhPho').on('change', function(){
        console.log($(this).val());
        for(var i = 0; i < myJson.country.length; i++)
        {
            if(myJson.country[i].id == $(this).val())
            {
                $('#quanHuyen').html('<option style="font-weight:bold" value="000">Chọn quận huyện</option>');
                $.each(myJson.country[i].huyen, function (index, value) {
                    $("#quanHuyen").append('<option value="'+value.id+'">'+value.name+'</option>');
                });
            }
        }
    });

    $('#quanHuyen').on('change', function(){
        console.log($(this).val());
        for(var i = 0; i < myJson.country.length; i++)
        {
            for(var j = 0; j < myJson.country[i].huyen.length; j++) {

                if(myJson.country[i].huyen[j].id == $(this).val())

                {
                    $('#xaPhuong').html('<option style="font-weight:bold" value="000">Chọn xã phường</option>');
                    $.each(myJson.country[i].huyen[j].xa, function (index, value) {
                        $("#xaPhuong").append('<option value="'+value.id+'">'+value.name+'</option>');
                    });
                }
            }
        }
    });
